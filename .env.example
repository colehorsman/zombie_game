# Sonrai API Configuration
SONRAI_API_URL=https://YOUR_ORG-graphql-server.sonraisecurity.com/graphql
SONRAI_ORG_ID=your_org_id_here
SONRAI_API_TOKEN=your_api_token_here

# AWS Account Filter (optional - leave blank to fetch all accounts)
AWS_ACCOUNT=
DAYS_SINCE_LAST_LOGIN=0

# Game Configuration
GAME_WIDTH=1280          # Base rendering resolution (width)
GAME_HEIGHT=720          # Base rendering resolution (height)
FULLSCREEN=false         # Start in fullscreen mode (true/false) - toggle with F11
TARGET_FPS=60
